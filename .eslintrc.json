{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "camelcase": "off",
    "import/extensions": "off",
    "no-alert": "off"
  },
  "ignorePatterns": ["node_modules"]
}
